<% include ../partials/header %>
<script>
    $(document).ready( function () {
        $('#table_id').DataTable();
    } ); 
</script>


<table id="table_id" class="display">
    <thead>
        <tr>
            <th>Date</th>
            <th>Name</th>
            <th>Item</th>
            <th>Rate</th>
            <th>Weight</th>
            <th>Price</th>
            <th>IsComplete</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        <% orders.forEach(function(order){ %>
        <tr>
            <td><%= order.date %></td>
            <td><%= order.customer.name %></td>
            <td><%= order.item %></td>
            <td><%= order.rate %></td>
            <td><%= order.weight %></td>
            <td><%= order.price %></td>
            <td><%= order.isComplete %></td>
            <td><%= order.total %></td>
        </tr>
        <% }) %>
    </tbody>
</table>

<% include ../partials/footer %>